<module rename-to='belphegor'>
    <!-- Inherit the core Web Toolkit stuff. -->
    <inherits name='com.google.gwt.user.theme.standard.Standard' />

    <inherits name='com.sencha.gxt.ui.GXT' />
    <stylesheet src="reset.css" />
    <inherits name='com.sencha.gxt.theme.gray.Gray' />
    <inherits name="com.google.common.collect.Collect"/>

    <inherits name="org.iplantc.de.admin.desktop.AdminDesktop" />

    <inherits name="org.iplantc.de.theme.base.DiskResource" />

    <replace-with class="org.iplantc.de.commons.client.appearance.widgets.IPlantAnchorDefaultAppearance">
        <when-type-is class="org.iplantc.de.commons.client.widgets.IPlantAnchorAppearance" />
    </replace-with>

    <!-- Specify the app entry point class. -->
    <entry-point class='org.iplantc.admin.belphegor.client.Belphegor' />

    <!-- Specify the paths for translatable code -->
    <source path='client' />
    <source path='shared' />
</module>
