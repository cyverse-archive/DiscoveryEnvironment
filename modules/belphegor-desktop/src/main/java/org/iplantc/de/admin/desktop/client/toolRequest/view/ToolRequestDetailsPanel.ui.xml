<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	         xmlns:g="urn:import:com.google.gwt.user.client.ui"
	         xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container"
             xmlns:toolbar="urn:import:com.sencha.gxt.widget.core.client.toolbar"
             xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client"
             xmlns:form="urn:import:com.sencha.gxt.widget.core.client.form">
    <ui:with field="margins"
             type="com.sencha.gxt.core.client.util.Margins">
      <ui:attributes top="0" right="5" bottom="10" left="5" />
    </ui:with>
    <ui:with field="layoutData"
             type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData">
      <ui:attributes width="1" height="-1" margins="{margins}" />
    </ui:with>

	<gxt:ContentPanel headingText="Details">
        <container:VerticalLayoutContainer ui:field="con">
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="Additional Data File">
                    <form:widget>
                        <form:TextField readOnly="true"
                        				enabled="false"
                                        ui:field="additionalDataFileEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="Additional Info">
                    <form:widget>
                        <form:TextField readOnly="true"
                        				enabled="false"
                                        ui:field="additionalInfoEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="Architecture">
                    <form:widget>
                        <form:TextField readOnly="true"
                        				enabled="false"
                                        ui:field="architectureEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="Attribution">
                    <form:widget>
                        <form:TextField readOnly="true"
                        				enabled="false"
                                        ui:field="attributionEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="Cmd Line Description">
                    <form:widget>
                        <form:TextField readOnly="true"
                        				enabled="false"
                                        ui:field="cmdLineDescriptionEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="Doc Url">
                    <form:widget>
                        <form:TextField readOnly="true"
                        				enabled="false"
                                        ui:field="documentationUrlEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:CheckBox ui:field="multiThreadedEditor"
                			   enabled="false"
                               boxLabel="Multi-Threaded"/>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="Phone">
                    <form:widget>
                        <form:TextField readOnly="true"
                        				enabled="false"
                                        ui:field="phoneEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="Source Url">
                    <form:widget>
                        <form:TextField readOnly="true"
                        				enabled="false"
                                        ui:field="sourceUrlEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="Submitted by">
                    <form:widget>
                        <form:TextField readOnly="true"
                        				enabled="false"
                                        ui:field="submittedByEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="Test Data Path">
                    <form:widget>
                        <form:TextField readOnly="true"
                        				enabled="false"
                                        ui:field="testDataPathEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
            <container:child layoutData="{layoutData}">
                <form:FieldLabel text="Version">
                    <form:widget>
                        <form:TextField readOnly="true"
                        				enabled="false"
                                        ui:field="versionEditor"/>
                    </form:widget>
                </form:FieldLabel>
            </container:child>
        
        </container:VerticalLayoutContainer>
      </gxt:ContentPanel>
</ui:UiBinder> 