<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container"
	xmlns:button="urn:import:com.sencha.gxt.widget.core.client.button"
	xmlns:toolbar="urn:import:com.sencha.gxt.widget.core.client.toolbar">
  <ui:with
        field="strings"
        type="org.iplantc.de.resources.client.messages.IplantDisplayStrings" />

<container:VerticalLayoutContainer ui:field="contents">
<toolbar:ToolBar ui:field="toolbar" height="30px">
            <button:TextButton
                text="{strings.simpleUploadForm}"
                ui:field="btnSimpleUpld"
                enabled="true"
                visible="false"/>
              <button:TextButton
                text="{strings.simpleDownloadForm}"
                ui:field="btnSimpleDwld"
                enabled="true"
                visible="false"/>
        </toolbar:ToolBar>
 </container:VerticalLayoutContainer>
</ui:UiBinder>
